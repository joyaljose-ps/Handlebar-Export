    <script>
        (function () {
            var progress;
            var size = 120;
            var strokeWidth = 6;
            const center = size / 2;
            const radius = center - strokeWidth;

            const circles = document.querySelectorAll('.svg-pi');
            for (var i = 0; i < circles.length; i++) {
                progress = circles[i].parentElement.getAttribute('data-percent');
                const arcLength = 2 * 3.14 * radius;
                const arcOffset = 311.2 * ((100 - parseFloat(progress)) / 100);

                circles[i].querySelector('.svg-pi-indicator').style['stroke-dashoffset'] = arcOffset
            }
        })();
    </script>
