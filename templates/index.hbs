                <img src="https://uploads-ssl.webflow.com/6302274a656c24095997e3c5/6332ba4b45f84aee411fda50_CIS.png"
                    class="h-9 w-9">
                {{/ifEquals}}
                <!--HIPAA-->
                {{#ifEquals compliance.id "HIPAA"}}
                <img src="https://uploads-ssl.webflow.com/6302274a656c24095997e3c5/6332ba4b45f84aee411fda50_CIS.png"
                    class="h-9 w-9">
                {{/ifEquals}}
                <!--MITRE_ATT&CK_v10_0-->
                {{#ifEquals compliance.id "MITRE_ATT&CK_v10_0"}}
                <img src="https://uploads-ssl.webflow.com/6302274a656c24095997e3c5/6332ba4b45f84aee411fda50_CIS.png"
                    class="h-9 w-9">
                {{/ifEquals}}
                <!--AWS_RBI_MD_ITF-->
                {{#ifEquals compliance.id "AWS_RBI_MD_ITF"}}
                <img src="https://uploads-ssl.webflow.com/6302274a656c24095997e3c5/6332ba4b45f84aee411fda50_CIS.png"
                    class="h-9 w-9">
                {{/ifEquals}}
                <!--AWS_RBI_UCB-->
                {{#ifEquals compliance.id "AWS_RBI_UCB"}}
                <img src="https://uploads-ssl.webflow.com/6302274a656c24095997e3c5/6332ba4b45f84aee411fda50_CIS.png"
                    class="h-9 w-9">
                {{/ifEquals}}
                <p class="pl-3 font-semibold text-[20px]" style="color:#000000">{{compliance.title}}</p>
            </div>
            <div class="lineDivide mt-16 w-full" style="height:1px"></div>

            <p class="text-[#444444] mt-8 text-[14px] font-normal">Project</p>
            <p class="mt-3 text-black text-base font-semibold">{{projects.0.title}}</p>

            <p class="text-[#444444] mt-8 text-[14px] font-normal">Issuer</p>
            <p class=" mt-3 text-black text-base font-semibold">{{user.name}}</p>

            <p class="text-[#444444] mt-8 text-[14px] font-normal">Date created</p>
            <p class=" mt-3 text-black text-base font-semibold">{{time}}</p>

